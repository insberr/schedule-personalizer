{
  "name": "schedule-personalizer-svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write src && prettier --write static",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "husky install",
    "test:watch": "jest --watch",
    "test": "jest",
    "update-staff-list": "ts-node --project scripts/tsconfig.json --esm scripts/update-staff-list.ts"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "@types/jest": "latest",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.11.18",
    "@vite-pwa/sveltekit": "^0.1.2",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.18.1",
    "husky": ">=6",
    "jest": "latest",
    "lint-staged": ">=10",
    "node-fetch": "^3.3.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "ts-jest": "latest",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-pwa": "^0.14.1"
  },
  "type": "module",
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@formkit/auto-animate": "^1.0.0-beta.5",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@karmaniverous/serify-deserify": "^1.3.4",
    "@macfja/serializer": "^1.1.2",
    "@sveltejs/adapter-static": "^1.0.0",
    "array-move": "^4.0.0",
    "date-fns": "^2.29.3",
    "immer": "^9.0.17",
    "json5": "^2.2.3",
    "lodash.merge": "^4.6.2",
    "nanoid": "^4.0.0",
    "stream-browserify": "^3.0.0",
    "svelte-fa": "^3.0.3",
    "svelte-french-toast": "^1.0.3",
    "svelty-picker": "^3.2.3",
    "title-case": "^3.0.3",
    "xml-js": "^1.6.11",
    "yup": "^0.32.11"
  },
  "lint-staged": {
    "src/**/*": "prettier --ignore-unknown --write",
    "static/**/*": "prettier --ignore-unknown --write",
    "scripts/**/*": "prettier --ignore-unknown --write",
    "tests/**/*": "prettier --ignore-unknown --write",
    "*.config.*": "prettier --ignore-unknown --write"
  }
}
