{
    "name": "schedule-personalizer",
    "version": "5.0.0",
    "description": "Personalized Schedule",
    "source": [
        "src/index.html"
    ],
    "repository": "https://github.com/insberr/schedule-personalizer.git",
    "author": "insberr & Wackery",
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "@parcel/core": "^2.7.0",
        "@parcel/packager-raw-url": "^2.7.0",
        "@parcel/packager-xml": "^2.7.0",
        "@parcel/transformer-mdx": "^2.7.0",
        "@parcel/transformer-sass": "^2.7.0",
        "@parcel/transformer-webmanifest": "^2.7.0",
        "@types/bootstrap": "^5.2.5",
        "@types/eslint": "^8.4.7",
        "@types/events": "^3.0.0",
        "@types/keyboardjs": "^2.5.0",
        "@types/lodash": "^4.14.186",
        "@types/node": "^18.11.3",
        "@types/pathfinding": "^0.0.6",
        "@types/react": "^18.0.21",
        "@types/react-calendar": "^3.5.3",
        "@types/react-dom": "^18.0.6",
        "@types/rimraf": "^3.0.2",
        "@types/serviceworker": "^0.0.53",
        "@types/sharp": "^0.31.0",
        "@types/stringify-object": "^4.0.1",
        "@types/styled-components": "^5.1.26",
        "@types/tinycolor2": "^1.4.3",
        "@types/workbox-precaching": "^4.3.1",
        "@types/workbox-sw": "^4.3.2",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "eruda": "^2.5.0",
        "eslint": "^8.25.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "events": "^3.3.0",
        "gh-pages": "^4.0.0",
        "parcel": "^2.7.0",
        "parcel-reporter-clean-dist": "^1.0.4",
        "parcel-reporter-static-files-copy": "^1.4.0",
        "rimraf": "^3.0.2",
        "sharp": "^0.29.1",
        "stream-browserify": "^3.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2",
        "typescript-plugin-css-modules": "^3.4.0"
    },
    "browserslist": "defaults and not ie >0 and not ie_mob >0",
    "scripts": {
        "host": "echo hi",
        "build": "parcel build --detailed-report",
        "build-gh-pages": "parcel build --public-url . --detailed-report",
        "dev": "parcel serve",
        "check": "tsc && eslint src",
        "clean": "rimraf dist && rimraf .parcel-cache",
        "splash": "cd src && pwa-asset-generator ../src/icons/icon.svg ./splashscreens --background #272727 --splash-only --index index.html --type png --padding \"calc(50vh - 20%) calc(50vw - 40%)\"",
        "fullbuild": "yarn splash && yarn build",
        "legal": "echo '# Licenses' > src/legal.mdx && yarn licenses generate-disclaimer --production >> src/legal.mdx",
        "postinstall": "yarn legal",
        "upgrade:latest:dont-use-because-of-parcel": "yarn up \"*\" \"@*/*\""
    },
    "packageManager": "yarn@3.2.3",
    "pwaManifest": {
        "name": "Schedule Personalizer",
        "shortName": "Schedule",
        "startURL": "./",
        "theme": "#272727",
        "genSafariPinnedTab": true,
        "appleTouchIconBG": "#272727",
        "generateIconOptions": {
            "baseIcon": "./icon.svg",
            "genFavicons": true,
            "purpose": [
                "any"
            ]
        },
        "icons": [
            {
                "src": "",
                "sizes": "250x250",
                "type": "image/png",
                "purpose": "maskable"
            }
        ]
    },
    "dependencies": {
        "@karmaniverous/serify-deserify": "^1.2.9",
        "@mdx-js/react": "^1.6.22",
        "@parcel/service-worker": "^2.7.0",
        "@popperjs/core": "^2.11.6",
        "@react-spring/web": "^9.5.5",
        "@redux-devtools/extension": "^3.2.3",
        "@reduxjs/toolkit": "^1.8.6",
        "@sentry/browser": "^7.16.0",
        "@sentry/react": "^7.16.0",
        "@sentry/rrweb": "^0.3.3",
        "@sentry/tracing": "^7.16.0",
        "bootstrap": "^5.2.2",
        "browserslist": "^4.21.4",
        "comlink": "^4.3.1",
        "copy-image-clipboard": "^2.1.2",
        "date-fns": "^2.29.3",
        "get-own-enumerable-property-symbols": "^3.0.2",
        "html-to-image": "^1.10.8",
        "is-obj": "^3.0.0",
        "is-regexp": "^3.1.0",
        "keyboardjs": "^2.7.0",
        "lodash": "^4.17.21",
        "pathfinding": "^0.4.18",
        "pepjs": "^0.5.3",
        "pwa-asset-generator": "^6.2.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.5.0",
        "react-calendar": "^3.9.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.6.0",
        "react-is": "^18.2.0",
        "react-redux": "^8.0.4",
        "react-use": "^17.4.0",
        "redux": "^4.2.0",
        "redux-persist": "^6.0.0",
        "rrweb": "^2.0.0-alpha.3",
        "stringify-object": "^4.0.1",
        "styled-components": "^5.3.6",
        "swr": "^1.3.0",
        "tinycolor2": "^1.4.2",
        "workbox-background-sync": "^6.5.4",
        "workbox-cacheable-response": "^6.5.4",
        "workbox-core": "^6.5.4",
        "workbox-precaching": "^6.5.4",
        "workbox-sw": "^6.5.4",
        "xml-js": "^1.6.11"
    },
    "alias": {
        "process": false
    },
    "staticFiles": {
        "staticPath": "src/static"
    },
    "resolutions": {
        "stringify-object@^4.0.1": "patch:stringify-object@npm%3A4.0.1#./.yarn/patches/stringify-object-npm-4.0.1-47b9ca5893.patch"
    }
}
