{
  "name": "schedule-personalizer",
  "version": "5.0.0",
  "description": "Personalized Schedule",
  "source": [
    "src/index.html",
    "src/editor/index.html",
    "src/test/index.html"
  ],
  "repository": "https://github.com/insberr/schedule-personalizer.git",
  "author": "insberr & Wackery",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/packager-xml": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-webmanifest": "2.7.0",
    "@types/bootstrap": "^5.1.10",
    "@types/eslint": "^8",
    "@types/events": "^3",
    "@types/keyboardjs": "^2",
    "@types/node": "^17.0.31",
    "@types/react": "^18",
    "@types/react-calendar": "^3",
    "@types/react-dom": "^18",
    "@types/rimraf": "^3",
    "@types/sharp": "^0",
    "@types/styled-components": "^5",
    "@types/tinycolor2": "^1",
    "@types/workbox-precaching": "^4.3.1",
    "@types/workbox-sw": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eruda": "^2.4.1",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "events": "^3.3.0",
    "parcel": "^2.7.0",
    "parcel-config-precache-manifest": "^0.0.4",
    "parcel-reporter-clean-dist": "^1.0.4",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.29.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "browserslist": "defaults and not ie >0 and not ie_mob >0",
  "scripts": {
    "build": "parcel build --detailed-report",
    "build-config-repo": "echo \"Set this up\"",
    "dev": "parcel serve",
    "check": "tsc && eslint src",
    "clean": "rimraf dist && rimraf .parcel-cache",
    "splash": "cd src && pwa-asset-generator ../icon.svg ./splashscreens --background #272727 --splash-only --index index.html --type png --padding \"calc(50vh - 20%) calc(50vw - 40%)\"",
    "fullbuild": "yarn splash && yarn build"
  },
  "packageManager": "yarn@3.2.0",
  "pwaManifest": {
    "name": "Schedule Personalizer",
    "shortName": "Schedule",
    "startURL": "./",
    "theme": "#272727",
    "genSafariPinnedTab": true,
    "appleTouchIconBG": "#272727",
    "generateIconOptions": {
      "baseIcon": "./icon.svg",
      "genFavicons": true,
      "purpose": [
        "any"
      ]
    },
    "icons": [
      {
        "src": "",
        "sizes": "250x250",
        "type": "image/png",
        "purpose": "maskable"
      }
    ]
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@karmaniverous/serify-deserify": "^1.2.9",
    "@parcel/transformer-svg-react": "^2.7.0",
    "@popperjs/core": "^2.11.5",
    "@react-spring/web": "^9.5.2",
    "@redux-devtools/extension": "^3.2.3",
    "@reduxjs/toolkit": "^1.8.4",
    "@sentry/react": "^7.12.1",
    "@sentry/rrweb": "^0.3.3",
    "@sentry/tracing": "^7.12.1",
    "@types/lodash": "^4.14.184",
    "@types/react-big-calendar": "^0.38.1",
    "bootstrap": "^5.2.0",
    "browserslist": "^4.20.3",
    "copy-image-clipboard": "^2.1.2",
    "date-fns": "^2.28.0",
    "get-own-enumerable-property-symbols": "^3.0.2",
    "html-to-image": "^1.10.6",
    "html2canvas": "^1.4.1",
    "is-obj": "^3.0.0",
    "is-regexp": "^3.1.0",
    "keyboardjs": "^2.6.4",
    "lodash": "^4.17.21",
    "pepjs": "^0.5.3",
    "pwa-asset-generator": "^6.1.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.5.0",
    "react-bootstrap": "^2.5.0",
    "react-calendar": "^3.7.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-use": "^17.4.0",
    "redux-persist": "^6.0.0",
    "rrweb": "^2.0.0-alpha.1",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "tinycolor2": "^1.4.2",
    "use-react-screenshot": "^3.0.0",
    "workbox-precaching": "^6.5.3",
    "workbox-sw": "^6.5.3",
    "xml-js": "^1.6.11"
  },
  "alias": {
    "process": false
  },
  "staticFiles": {
    "staticPath": "src/static"
  }
}
